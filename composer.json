{
    "name": "imjoehaines/flowder-validator",
    "description": "A validator for Flowder fixture files",
    "keywords": [
        "fixtures",
        "fixture-loading",
        "mysql",
        "sqlite",
        "testing"
    ],
    "homepage": "https://github.com/imjoehaines/flowder-validator",
    "license": "The Unlicense",
    "authors": [
        {
            "name": "Joe Haines",
            "email": "hello@joehaines.co.uk",
            "homepage": "https://www.joehaines.co.uk",
            "role": "Developer"
        }
    ],
    "scripts": {
        "test": "phpunit tests/"
    },
    "config": {
        "platform": {
            "php": "5.6"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Imjoehaines\\Flowder\\Validator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Imjoehaines\\Flowder\\Validator\\Test\\": "tests/"
        }
    }
}
